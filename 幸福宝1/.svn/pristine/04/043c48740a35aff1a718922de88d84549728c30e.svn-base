
.inputs{
	background: white;
	height: 100rpx;
	padding: 0 30rpx;
	box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-size: 28rpx;
	color: #666666;
	border-top: 1rpx solid #DEDEDE;
}
.inpus{
	color: #333333;
}
.input{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.icon{
	width: 32rpx;
	height: 24rpx;
}

