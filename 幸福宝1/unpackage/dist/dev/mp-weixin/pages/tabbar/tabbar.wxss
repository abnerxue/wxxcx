
.bottomtabs{
	position: relative;
	width: 100%;
	height: 100rpx;
}
.bottomtab {
	position: fixed;
	bottom: 0;
	width: 100%;
	height: 100rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-justify-content: space-around;
	        justify-content: space-around;
	background: white;
	z-index: 55;
}
.bottabbar {
	height: 100%;
	width: 180rpx;
	font-size: 24rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	padding: 14rpx 0;
	box-sizing: border-box;
}
.ims {
	width: 40rpx;
	height: 40rpx;
}
.actives {
	color: #74181B;
	position: relative;
}
.bottabbar:nth-child(2) view:nth-child(2){
	position: absolute;
	z-index: 1;
	bottom: 14rpx;
}
.radiu{
	width: 108rpx;
	height: 108rpx;
	border-radius: 50%;
	background: white;
	position: absolute;
	top: -30rpx;
	left: 0;
	right: 0;
	margin: 0 auto;
	z-index: 0;
	text-align: center;
}
.radiu image{
	width: 72rpx;
	height: 72rpx;
	border-radius: 50%;
	margin-top: 12rpx;
}

