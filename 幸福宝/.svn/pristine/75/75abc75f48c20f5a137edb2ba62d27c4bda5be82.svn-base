
.bottomtabs{
	position: relative;
	width: 100%;
	height: 100rpx;
}
.bottomtab {
	position: fixed;
	bottom: 0;
	width: 100%;
	height: 100rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-justify-content: space-around;
	        justify-content: space-around;
	background: white;
	z-index: 55;
}
.bottabbar {
	height: 100%;
	width: 180rpx;
	font-size: 24rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	padding: 14rpx 0;
	box-sizing: border-box;
}
.ims {
	width: 40rpx;
	height: 40rpx;
}
.actives {
	color: #74181B;
}
