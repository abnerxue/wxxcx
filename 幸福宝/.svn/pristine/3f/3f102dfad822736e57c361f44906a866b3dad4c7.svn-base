
.back_image{
	background: url(../../static/img/goye.65ed24f6.png) no-repeat;
	width: 100%;
	background-size: 100%;
	height: 421.88rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	color: white;
	font-size: 60rpx;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.title{
	margin: 0 30rpx;
}
.content_list{
	width: 100%;
	padding: 20rpx 32rpx 0;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	box-sizing: border-box;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.content{
	width: 327rpx;
	height: 288rpx;
	background: white;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	margin-bottom: 20rpx;
	border-radius: 10rpx;
	box-shadow: 0 4rpx 16rpx #EDEBEB;
}
.content_image{
	width: 100%;
	height: 216rpx;
}
.content_font{
	width: 100%;
	height: 72rpx;
